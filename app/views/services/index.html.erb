<div class="service-container">
  <h3 class="service-header"><%= link_to(image_tag("arrow.png", class: 'img-arrow'), root_path,  :method => :get) %>Services</h3>
</div>

<div
  id="map"
  style="width: 100%;
  height: 190px;"
  data-markers="<%= @markers.to_json %>"
></div>


<div>
   <% @services.each do |service|%>
    <div class="col-xs-6">
      <div class="services-card">

        <h2 class="card-category" > <%= service.category %> </h2>
       </div>
    </div>
   <% end %>
</div>



<div class = "service-header">
  <h1>Services</h1>
</div>

<div
  id="map"
  style="width: 100%;
  height: 255px;"
  data-markers="<%= @markers.to_json %>"
></div>


<div>
   <% @services.each do |service|%>
    <div class="col-xs-6">
      <div class="services-card">

        <h2 class="card-category" > <%= service.category %> </h2>
       </div>
    </div>
   <% end %>
</div>


